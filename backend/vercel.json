{
    "version": 2,
    "builds": [
      {
        "src": "api/index.ts",
        "use": "@vercel/node"
      },
      {
        "src": "api/cron/cleanup.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/cron/cleanup", "dest": "api/cron/cleanup.ts" },
      { "src": "/api/v1/booking", "dest": "api/index.ts" },
      { "src": "/api/v1/bookingBasedOnCategory", "dest": "api/index.ts" },
      {
        "src": "/ping",
        "dest": "api/index.ts"
      }
    ]
  }
  